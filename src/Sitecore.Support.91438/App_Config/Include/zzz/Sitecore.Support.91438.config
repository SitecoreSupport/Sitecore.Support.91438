<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set/">
  <sitecore>
    <pipelines>

      <renderLayout>
        <processor type="Sitecore.Pipelines.RenderLayout.SecurityCheck, Sitecore.Kernel" >
          <patch:attribute name="type">Sitecore.Support.Pipelines.RenderLayout.SecurityCheck, Sitecore.Support.91438</patch:attribute>
        </processor>
      </renderLayout>

      <httpRequestBegin>
        <processor type="Sitecore.Pipelines.HttpRequest.ExecuteRequest, Sitecore.Kernel">
          <patch:attribute name="type">Sitecore.Support.Pipelines.HttpRequest.ExecuteRequest, Sitecore.Support.91438</patch:attribute>
        </processor>
      </httpRequestBegin>

    </pipelines>
  </sitecore>
</configuration>